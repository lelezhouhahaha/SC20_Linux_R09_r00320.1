//============================================================================+
/** @COMMAND MCM_LOC_SET_INDICATIONS
    @BRIEF Registers for location indications.
    @DESCRIPTION
        Corresponding indications to register for or deregister from: \n
        location_info, status_info, sv_info, nmea_info, capabilities_info,
        utc_time_req, xtra_data_req, agps_data_conn_cmd_req,
        ni_notify_user_response_req, xtra_report_server.
    @CMD_VERSION 1.0
    @USAGE Production
    @SECURITY_LEVEL  High risk
    @REQUIRED_ACTION Default
*/
//============================================================================


//! @MSG     MCM_LOC_SET_INDICATIONS_REQ
//! @TYPE    Request
//! @SENDER  Control point
//----------------------------------------------------------------------------
message {
    mandatory boolean register_location_info_ind;  //!< Register for location information indications.
    mandatory boolean register_status_info_ind;  //!< Register for status information indications.
    mandatory boolean register_sv_info_ind;  //!< Register for SV information indications.
    mandatory boolean register_nmea_info_ind;  //!< Register for NMEA information indications.
    mandatory boolean register_capabilities_info_ind;  //!< Register for capabilities information indications.
    mandatory boolean register_utc_time_req_ind;  //!< Register for UTC time request indications.
    mandatory boolean register_xtra_data_req_ind;  //!< Register for XTRA data request indications.
    mandatory boolean register_agps_data_conn_cmd_req_ind;  //!< Register for AGPS data connection command request indications.
    mandatory boolean register_ni_notify_user_response_req_ind;  //!< Register for NI notify user response request indications.
    optional boolean register_xtra_report_server_ind;  //!< Register for XTRA report server indications.
} mcm_loc_set_indications_req_msg;


//! @MSG     MCM_LOC_SET_INDICATIONS_RESP
//! @TYPE    Response
//! @SENDER  Service
//----------------------------------------------------------------------------
message {
  //! Result Code
  //! @VERSION_INTRODUCED 1.0
  //! @VERSION 1.0
  mandatory mcm_response_t resp;  //!< Result code.
} mcm_loc_set_indications_resp_msg;

/** @ERROR
    -MCM_ERROR_NONE              No error in the request
    -MCM_ERROR_INTERNAL          Unexpected error occurred during processing
    -MCM_ERROR_MALFORMED_MSG     Message was not formulated correctly by the
                               control point or the message was corrupted
                               during transmission
    -MCM_ERROR_MISSING_ARG       One or more required TLVs were missing in the request
    -MCM_ERROR_INVALID_HANDLE    Handle provided in the request
                               is not valid, i.e., it is not assigned to the
                               control point
    -MCM_ERROR_INVALID_ARG       Argument is not correct
    -MCM_ERROR_NOT_SUPPORTED     Operation is not supported
*/

