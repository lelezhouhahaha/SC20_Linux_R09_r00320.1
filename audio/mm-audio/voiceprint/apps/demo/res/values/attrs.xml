<resources>

    <!-- For custom list view -->
    <declare-styleable name="PhraseListView">
        <attr name="mode" format="enum">
            <enum name="select" value="0"/>
            <enum name="edit" value="1"/>
        </attr>
    </declare-styleable>

    <!-- Mic Fragment -->
    <declare-styleable name="MicFragmentMode">
        <attr name="mic_mode" format="enum">
            <enum name="regular" value="0"/>
            <enum name="error" value="1"/>
        </attr>
    </declare-styleable>



</resources>
